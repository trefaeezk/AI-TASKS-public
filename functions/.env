# متغيرات البيئة الخاصة بوظائف Firebase

# مفتاح API لخدمة Resend لإرسال البريد الإلكتروني
RESEND_API_KEY="YOUR_RESEND_API_KEY_HERE"

# مفتاح API لخدمة Google Generative AI
GOOGLE_GENAI_API_KEY="YOUR_GOOGLE_GENAI_API_KEY_HERE"

# إعدادات SMTP (اختياري، إذا كنت تستخدم SMTP لإرسال البريد)
# SMTP_HOST="smtp.example.com"
# SMTP_PORT="587"
# SMTP_USER="your_email@example.com"
# SMTP_PASS="your_app_password"

# رابط التطبيق الأساسي (يستخدم في روابط الدعوات وغيرها)
# تأكد من تحديث هذا الرابط ليعكس بيئة الإنتاج أو التطوير الخاصة بك
APP_BASE_URL="http://localhost:9003"
